{
  "name": "sql-in-ts",
  "version": "0.0.13",
  "main": "src/index.ts",
  "files": ["src/**"],
  "bin": "out/cli_generator.js",
  "dependencies": {
    "@js-joda/core": "^5.3.1",
    "lodash": "^4.17.21",
    "trader-pgsql-ast-parser": "10.5.2",
    "postgres": "^3.3.1"
  },
  "scripts": {
    "check": "tsc -p . --noEmit",
    "build": "node esbuild.js",
    "prepublishOnly": "npm run build",
    "start": "node esbuild.js && node ./out/index.js",
    "example": "node esbuild.js && node ./out/example.js",
    "example_aperi": "node esbuild.js && node ./out/aperi_index.js",
    "example_debug": "node esbuild.js && node --inspect-brk ./out/example.js",
    "debug": "node esbuild.js && node --inspect-brk ./out/index.js",
    "gen": "node esbuild.js && node ./out/cli/cli_generator.js -f src/demo.sql -o out/demo.ts",
    "gen_aperi": "node esbuild.js && node ./out/cli/cli_generator.js -f ./aperi.sql -o out/aperi.ts"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^14.14.10",
    "esbuild": "0.8.34",
    "prettier": "^2.7.1",
    "ts-node": "^10.7.0",
    "typescript": "4.9.4"
  }
}
